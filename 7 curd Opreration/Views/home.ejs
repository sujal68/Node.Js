        <%allUsers.map((User , index)=>{ %>
            <tr>
                <td>
                    <%= User.Id %>
                </td>
                <td>
                    <%= User.Name %>
                </td>
                <td>
                    <%= User.Email %>
                </td>
                <td>
                    <%= User.Password %>
                </td>
                <td>
                    <%= User.Phone %>
                </td>
                <td>
                    <%= User.Address %>
                </td>
                <td>
                    <%= User.Age %>
                </td>
            </tr>
            <% })%>
    </table>
</body>

</html>